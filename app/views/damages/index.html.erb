<p id="notice"><%= notice %></p>

<h1>Damages</h1>

<table>
  <thead>
    <tr>
      <th>State</th>
      <th>Primary</th>
      <th>Secondary</th>
      <th>Value</th>
      <th>Details</th>
      <th>Vehicle</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @damages.each do |damage| %>
      <tr>
        <td><%= damage.state %></td>
        <td><%= damage.primary %></td>
        <td><%= damage.secondary %></td>
        <td><%= damage.value %></td>
        <td><%= damage.details %></td>
        <td><%= damage.vehicle %></td>
        <td><%= damage.user %></td>
        <td><%= link_to 'Show', damage %></td>
        <td><%= link_to 'Edit', edit_damage_path(damage) %></td>
        <td><%= link_to 'Destroy', damage, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Damage', new_damage_path %>
